{
  "name": "snugug-blog",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "predev": "run-s import",
    "dev": "astro dev",
    "prestart": "run-s import",
    "start": "astro dev",
    "prebuild": "run-s import",
    "build": "astro build",
    "preview": "astro preview",
    "prepare": "cd ../ && husky install ./site/.husky",
    "import": "node ./lib/tweetback/import.js",
    "astro": "astro",
    "pretest": "run-s lint",
    "test": "run-s build",
    "lint": "run-p *:lint",
    "lint:staged": "lint-staged",
    "fix": "run-p *:fix",
    "js:lint": "eslint --quiet .",
    "js:fix": "eslint --fix .",
    "prettier:lint": "prettier --plugin-search-dir=. -c .",
    "prettier:fix": "prettier --plugin-search-dir=. --write .",
    "css:lint": "stylelint \"**/*.{css,scss,svelte,astro}\"",
    "css:fix": "stylelint --fix \"**/*.{css,scss,svelte,astro}\"",
    "import:twitter-archive": "node lib/tweetback/import.js"
  },
  "dependencies": {
    "@astrojs/rss": "^1.0.3",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/svelte": "^1.0.2",
    "@sanity/client": "^3.4.1",
    "astro": "^1.6.0",
    "chromeos-dev-markdown": "^1.0.1",
    "css-paint-polyfill": "^3.4.0",
    "svelte": "^3.52.0",
    "the-new-css-reset": "^1.7.3",
    "vite-plugin-pwa": "^0.13.1",
    "workbox-precaching": "^6.5.4"
  },
  "devDependencies": {
    "@11ty/eleventy-img": "^2.0.1",
    "@sanity/webhook": "^2.0.0",
    "@tweetback/canonical": "^2.0.3",
    "@typescript-eslint/parser": "^5.42.0",
    "cli-progress": "^3.11.2",
    "eslint": "^8.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.21.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.1",
    "letterform-shades": "^1.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "parse-domain": "^7.0.1",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.7.0",
    "prettier-plugin-svelte": "^2.8.1",
    "sass": "^1.55.0",
    "sentiment": "^5.0.2",
    "sqlite3": "^5.1.2",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0"
  },
  "lint-staged": {
    "**/*.{css,scss,svelte,astro}": "stylelint",
    "**/*.{js,mjs,cjs,ts,json,astro,svelte}": "eslint",
    "**/*": "prettier --plugin-search-dir=. -c"
  },
  "volta": {
    "node": "18.12.0"
  }
}
