{
  "extends": "astro/tsconfigs/base",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$components/*": ["src/components/*"],
      "$layouts/*": ["src/layouts/*"],
      "$sass/*": ["src/sass/*"],
      "$js/*": ["src/js/*"],
      "$lib/*": ["lib/*"]
    }
  }
}
